rename birthyr birthyear
merge m:1 sex race birthyear edcommon89 using $data\edrank_xw89
gen pop_hi75_1=(1-p_lo25)*perwt
gen obs_hi75_1=(1-p_lo25)*obs
gen pop_hi75_0=(p_lo25)*perwt
gen obs_hi75_0=(p_lo25)*obs

